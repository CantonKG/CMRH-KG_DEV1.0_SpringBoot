    </owl:NamedIndividual>
    


    <!-- http://www.owl-ontologies.com/Event.owl#中国共产党第一次全国代表大会 -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Event.owl#中国共产党第一次全国代表大会">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Event.owl#政治事件"/>
        <Event:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#李大钊"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1921年7月，中国共产党第一次全国代表大会召开，宣告中国共产党成立，从此中国革命的面貌为之一新。</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.owl-ontologies.com/Event.owl#举行大规模街头演讲 -->

