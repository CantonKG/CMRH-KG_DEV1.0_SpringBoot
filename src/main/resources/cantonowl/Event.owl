        <Event:父事件 rdf:resource="http://www.owl-ontologies.com/Event.owl#五四运动"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1919年5月7日，上海60多个团体举行国民大会。</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.owl-ontologies.com/Event.owl#上海各学校全部罢课 -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Event.owl#上海各学校全部罢课">
